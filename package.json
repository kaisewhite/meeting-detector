{
  "name": "meeting-detector",
  "version": "1.0.0",
  "description": "Real-time meeting detection for macOS desktop apps",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "meeting-detect.sh",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "watch": "tsc -w",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["meeting", "detection", "macos", "tcc", "privacy"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {},
  "os": ["darwin"],
  "engines": {
    "node": ">=14.0.0"
  }
}